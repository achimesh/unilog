{
    "compileOnSave": false,
    "compilerOptions": {
        "target": "ES2020",
        "moduleResolution": "Node",
        "strict": true,
        "declaration": true,
        "noImplicitOverride": true,
        "noUnusedLocals": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true
    },
    "exclude": [
        "node_modules"
    ]
}